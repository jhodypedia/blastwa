<div class="cardx">
  <h3>Users Management</h3>
  <table>
    <thead>
      <tr><th>ID</th><th>Username</th><th>Email</th><th>Role</th><th>API Key</th></tr>
    </thead>
    <tbody>
      <% users.forEach(u=>{ %>
      <tr>
        <td><%= u.id %></td>
        <td><%= u.username %></td>
        <td><%= u.email %></td>
        <td><%= u.role %></td>
        <td><%= u.apikey %></td>
      </tr>
      <% }) %>
    </tbody>
  </table>
</div>
